<div class="padded">
  
  <div class="row" id="new_object_opener">  
    <div class="pull-right">
      <button data-toggle="button" class="btn blue form_opener"  style="cursor:pointer;">New Employee</button>
      <!-- <a href="#modal" data-toggle="modal" class="btn">Launch Modal</a> -->
    </div>   
  </div>
  
   
  <%= render :partial => "offices/new_employee_form", :locals => {
    :new_employee => @new_employee,
    :url => create_employee_url  
  }%>
 
  
  
  
  
  
  <table class="table table-bordered table-striped table-header" id="object_list">
    <thead>
      <tr>
        <th>Name</th>
        <th>Admin</th>
        <th>Head PM</th>
        <th>Employee</th> 
      </tr>
    </thead>
    <tbody id="employee_list">
      <% @employees.each do |employee| %>
        <%= render :partial => "offices/employee_list_entry", :locals => {
          :employee => employee 
        }%>
        
      <% end %> 
    </tbody>
  </table>
</div>