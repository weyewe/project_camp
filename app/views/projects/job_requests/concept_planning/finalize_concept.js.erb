$(document).ready(function(){
  
  
  $("#finalizer").hideLoading();
  <% if @project.is_concept_finalized == true %>
    <% new_notification = escape_javascript(render :partial => "projects/job_requests/concept_planning/concept_finalized", 
      :locals => {
      :project => @project 
    }) %>
    
    $("#finalizer").replaceWith('<%= new_notification %>' );
  <% else %>
   
    <% new_form = escape_javascript(render :partial => "projects/job_requests/concept_planning/finalize_concept", 
      :locals => {
      :project => @project 
    }) %>
    
    $("#finalizer").replaceWith('<%= new_form %>' );
  <% end %>
  
});