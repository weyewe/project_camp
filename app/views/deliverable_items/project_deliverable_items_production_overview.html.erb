<% content_for :additional_mini_app_nav do %>
  <%= render :partial => 'layouts/mini_app_nav', :locals => {
    :view_page => :summary
  } %>
<% end %>

<% content_for :additional_js do %> 
  <%= javascript_include_tag "project_summary" %>
<% end %>



<div class="padded">
  
  
  
  
   
  <% if @deliverable_items.length == 0 %>
    <%= render :partial => "layouts/no_object_list", :locals => {
      :objects => "deliverable items",
      :single_object => "deliverable item" ,
      :subject => "project"
    } %>
  <% end %>
   
   
  <div class="row"> 
    <div class='span8' id="package_detail_wrapper">


      <% @deliverable_items.each do |deliverable_item| %>
        <%= render :partial => "projects/deliverable_item_production_progress", :locals => {
          :deliverable_item => deliverable_item 
        }%>

      <% end %>
    </div> 
  </div>

</div>